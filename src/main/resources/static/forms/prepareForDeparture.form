      },
      "id": "Field_1ytyp88",
      "key": "email",
      "validate": {
        "required": true,
        "minLength": 6,
        "maxLength": 320,
        "pattern": "^[\\w\\-\\.]+@([\\w-]+\\.)+[\\w-]{2,}$"
      },
      "disabled": false,
      "description": "Your email adress"
    }
